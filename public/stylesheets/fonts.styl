//
// FONTS
//

@font-face
  font-family 'entypo_socialregular'
  src url('entypo-social-webfont.eot')
  src url('entypo-social-webfont.eot?#iefix') format('embedded-opentype'),
         url('entypo-social-webfont.woff') format('woff'),
         url('entypo-social-webfont.ttf') format('truetype'),
         url('entypo-social-webfont.svg#entypo_socialregular') format('svg')
  font-weight normal
  font-style normal
